{
  "base": "semantic/",
  "paths": {
    "source": {
      "config": "src/theme.config",
      "definitions": "src/definitions/",
      "site": "src/site",
      "themes": "src/themes/"
    },
    "output": {
      "packaged": "../public_static/lib/semantic/",
      "uncompressed": "dist/components/",
      "compressed": "dist/components/",
      "themes": "../public_static/lib/semantic/themes/"
    },
    "clean": "../public_static/lib/semantic/"
  },
  "permission": 744,
  "autoInstall": true,
  "rtl": false,
  "components": [
    "reset", "site",
    "button", "container", "divider", "header",
    "icon", "image", "input", "label", "list", "loader",
    "rail", "reveal", "segment", "breadcrumb",
    "form", "grid", "menu", "message", "table", "card",
    "comment", "feed", "item", "checkbox",
    "dropdown", "embed", "modal", "popup", "progress",
    "rating", "search", "sidebar", "sticky", "tab", "transition",
    "api", "form", "state", "visibility", "statistic"
  ],
  "version": "2.3.1"
}